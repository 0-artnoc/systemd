[Unit]
Description=Virtual Machine and Container Download Service
Documentation=man:systemd-importd.service(8)

[Service]
ExecStart=@rootlibexecdir@/systemd-importd
BusName=org.freedesktop.import1
WatchdogSec=1min
PrivateTmp=yes
PrivateDevices=yes
ProtectSystem=full
ProtectHome=yes
